{
	"name": "dataflow_cahcesink",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference"
					},
					"name": "empdb"
				},
				{
					"dataset": {
						"referenceName": "Empdataset",
						"type": "DatasetReference"
					},
					"name": "empblob"
				}
			],
			"sinks": [
				{
					"name": "cachedmaxempid"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "surrogateKey1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "SELECT"
				}
			],
			"scriptLines": [
				"source(output(",
				"          maxid as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'select max(id) as maxid from customer',",
				"     format: 'query') ~> empdb",
				"source(output(",
				"          EMPLOYEE_ID as integer,",
				"          FIRST_NAME as string,",
				"          LAST_NAME as string,",
				"          EMAIL as string,",
				"          PHONE_NUMBER as string,",
				"          HIRE_DATE as string,",
				"          JOB_ID as string,",
				"          SALARY as integer,",
				"          COMMISSION_PCT as string,",
				"          MANAGER_ID as string,",
				"          DEPARTMENT_ID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> empblob",
				"empblob keyGenerate(output(EMPID as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"surrogateKey1 derive(EMPID = EMPLOYEE_ID + cachedmaxempid#outputs()[1].maxid) ~> derivedColumn1",
				"derivedColumn1 select(mapColumn(",
				"          FIRST_NAME,",
				"          empid = EMPID,",
				"          JOB_ID,",
				"          SALARY",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SELECT",
				"empdb sink(validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     store: 'cache',",
				"     format: 'inline',",
				"     output: false,",
				"     saveOrder: 1,",
				"     mapColumn(",
				"          maxid",
				"     )) ~> cachedmaxempid",
				"SELECT sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Name as string,",
				"          ID as integer,",
				"          LOC as string,",
				"          SAL as decimal(10,2)",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          Name = FIRST_NAME,",
				"          ID = empid,",
				"          LOC = JOB_ID,",
				"          SAL = SALARY",
				"     )) ~> sink1"
			]
		}
	}
}